<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.layout.*?>
<GridPane xmlns:fx="http://javafx.com/fxml/1" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity"
          minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0"
          xmlns="http://javafx.com/javafx/8" fx:controller="telas.Disponivel">
    <columnConstraints>
        <ColumnConstraints maxWidth="121.0" minWidth="10.0" prefWidth="85.0"/>
        <ColumnConstraints maxWidth="136.0" minWidth="10.0" prefWidth="90.0"/>
        <ColumnConstraints maxWidth="144.0" minWidth="10.0" prefWidth="141.0"/>
        <ColumnConstraints maxWidth="101.0" minWidth="10.0" prefWidth="84.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <children>
        <Label text="Dia - Início" textAlignment="RIGHT" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
        <MenuButton fx:id="botaoDiaInicio" mnemonicParsing="false" text="Escolher Dia - Início" wrapText="true"
                    GridPane.columnIndex="2" GridPane.rowIndex="2"/>
        <Button fx:id="botaoSalvar" mnemonicParsing="false" onAction="#salva" text="Salvar" textAlignment="CENTER"
                GridPane.columnIndex="1" GridPane.rowIndex="4"/>
        <Button fx:id="botaoVoltar" mnemonicParsing="false" onAction="#volta" text="Voltar" GridPane.columnIndex="2"
                GridPane.rowIndex="4"/>
    </children>
</GridPane>
